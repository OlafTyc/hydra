	author = {Stein, Lincoln D and Mungall, Christopher and Shu, ShengQiang and Caudy, Michael and Mangone, Marco and Day, Allen and Nickerson, Elizabeth and Stajich, Jason E and Harris, Todd W and Arva, Adrian and Lewis, Suzanna},
