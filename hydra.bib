	keywords = {Cladosporium, DNA, Fungal, Evolution, Molecular, Fungal Proteins, Gene Frequency, Gene Regulatory Networks, Genes, Fungal, Genetic Variation, Haplotypes, Lycopersicon esculentum, Molecular Sequence Data, Polymorphism, Genetic, Recombination, Genetic, Virulence},
