@article{klee_evaluating_2005-1,
	title = {Evaluating eukaryotic secreted protein prediction},
	volume = {6},
	issn = {1471-2105},
	url = {http://www.biomedcentral.com/1471-2105/6/256},
	doi = {10.1186/1471-2105-6-256},
	abstract = {BACKGROUND:Improvements in protein sequence annotation and an increase in the number of annotated protein databases has fueled development of an increasing number of software tools to predict secreted proteins. Six software programs capable of high throughput and employing a wide range of prediction methods, SignalP 3.0, SignalP 2.0, TargetP 1.01, PrediSi, Phobius, and ProtComp 6.0, are evaluated.RESULTS:Prediction accuracies were evaluated using 372 unbiased, eukaryotic, SwissProt protein sequences. TargetP, SignalP 3.0 maximum S-score and SignalP 3.0 D-score were the most accurate single scores (90-91\% accurate). The combination of a positive TargetP prediction, SignalP 2.0 maximum Y-score, and SignalP 3.0 maximum S-score increased accuracy by six percent.CONCLUSION:Single predictive scores could be highly accurate, but almost all accuracies were slightly less than those reported by program authors. Predictive accuracy could be substantially improved by combining scores from multiple methods into a single composite prediction.},
	number = {1},
	urldate = {2009-09-03},
	journal = {BMC Bioinformatics},
	author = {Klee, Eric and Ellis, Lynda},
	year = {2005},
	pages = {256}
}

@article{mikheenko_metaquast:_2015,
	title = {{MetaQUAST}: evaluation of metagenome assemblies},
	issn = {1367-4803, 1460-2059},
	shorttitle = {{MetaQUAST}},
	url = {http://bioinformatics.oxfordjournals.org/content/early/2015/11/26/bioinformatics.btv697},
	doi = {10.1093/bioinformatics/btv697},
	abstract = {Summary: During the past years we have witnessed the rapid development of new metagenome assembly methods. While there are many benchmark utilities designed for single-genome assemblies, there is no well-recognised evaluation and comparison tool for metagenomic-specific analogues. In this paper we present MetaQUAST, a modification of QUAST, the state-of-the-art tool for genome assembly evaluation based on alignment of contigs to a reference. MetaQUAST addresses such metagenome datasets features as (1) unknown species content by detecting and downloading reference sequences, (2) huge diversity by giving comprehensive reports for multiple genomes, (3) presence of highly relative species by detecting chimeric contigs. We demonstrate MetaQUAST performance by comparing several leading assemblers on one simulated and two real datasets.
Availability: http://bioinf.spbau.ru/metaquast
Contact: gurevich@bioinf.spbau.ru
Supplementary Information: Available at Bioinformatics online.},
