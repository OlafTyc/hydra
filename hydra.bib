	journal = {Bioinformatics},
	author = {Pagni, Marco and Niculita-Hirzel, Helene and Pellissier, Loic and Dubuis, Anne and Xenarios, Ioannis and Guisan, Antoine and Sanders, Ian R. and Goudet, Jerome and Guex, Nicolas},
	month = may,
	year = {2013},
	pmid = {23539304},
	pmcid = {PMC3654712},
	pages = {1268--1274}
}

