	month = jan,
	year = {2014},
	keywords = {Environmental Science and Engineering, Illumina, Metagenomics, Microbial ecology, Microbiology, Roche 454},
	pages = {183--201},
	file = {Snapshot:/home/mattias/.mozilla/firefox/88hqpn07.default/zotero/storage/XNSD8ZMB/978-1-62703-712-9_15.html:text/html}
}

