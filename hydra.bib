	note = {00012 },
	pages = {i349--i355},
	file = {Full Text PDF:/home/mattias/.mozilla/firefox/88hqpn07.default/zotero/storage/QW5F8N7W/Hauswedell et al. - 2014 - Lambda the local aligner for massive biological d.pdf:application/pdf;Snapshot:/home/mattias/.mozilla/firefox/88hqpn07.default/zotero/storage/ZS458HGC/i349.html:text/html}
