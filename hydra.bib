	pmid = {15846337},
	keywords = {Fungal Proteins, Genes, Fungal, Genome, Fungal, Guanosine Diphosphate, Guanosine Triphosphate, Magnaporthe, Oryza sativa, Plant Diseases, Point Mutation, Proteome, Receptors, G-Protein-Coupled, Repetitive Sequences, Nucleic Acid, Saccharomyces cerevisiae, Signal Transduction, Virulence},
