	pmid = {17343675},
	keywords = {Algal Proteins, Ascomycota, Basidiomycota, Cell Membrane, Fungal Proteins, Oomycetes, Plant Diseases, Plants, Virulence},
	pages = {181--188}
}

@article{de_bie_cafe:_2006,
