@article{kunin_bioinformaticians_2008,
	title = {A bioinformatician's guide to metagenomics},
	volume = {72},
	issn = {1098-5557},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/19052320},
	doi = {10.1128/MMBR.00009-08},
	abstract = {As random shotgun metagenomic projects proliferate and become the dominant source of publicly available sequence data, procedures for the best practices in their execution and analysis become increasingly important. Based on our experience at the Joint Genome Institute, we describe the chain of decisions accompanying a metagenomic project from the viewpoint of the bioinformatic analysis step by step. We guide the reader through a standard workflow for a metagenomic project beginning with presequencing considerations such as community composition and sequence data type that will greatly influence downstream analyses. We proceed with recommendations for sampling and data generation including sample and metadata collection, community profiling, construction of shotgun libraries, and sequencing strategies. We then discuss the application of generic sequence processing steps (read preprocessing, assembly, and gene prediction and annotation) to metagenomic data sets in contrast to genome projects. Different types of data analyses particular to metagenomes are then presented, including binning, dominant population analysis, and gene-centric analysis. Finally, data management issues are presented and discussed. We hope that this review will assist bioinformaticians and biologists in making better-informed decisions on their journey during a metagenomic project.},
	number = {4},
	urldate = {2010-03-07},
	journal = {Microbiology and Molecular Biology Reviews: MMBR},
	author = {Kunin, Victor and Copeland, Alex and Lapidus, Alla and Mavromatis, Konstantinos and Hugenholtz, Philip},
	month = dec,
	year = {2008},
	pmid = {19052320},
	keywords = {Database Management Systems, Databases, Genetic, Environmental Microbiology, Genome, Genomics, Information Storage and Retrieval, Sequence Analysis, DNA},
	pages = {557--578, Table of Contents}
}

@article{porras-alfaro_diversity_2010,
	title = {Diversity and distribution of soil fungal communities in a semiarid grassland},
	volume = {2010},
	url = {http://www.mycologia.org/cgi/content/abstract/09-297v1},
	doi = {10.3852/09-297},
	abstract = {The fungal loop model of semiarid ecosystems integrates microtopographic structure and pulse dynamics with key microbial processes. However, limited data exist about the composition and structure of fungal communities in these ecosystems. The goal of this study was to characterize diversity and structure of soil fungal communities in semiarid grassland. The effect of long-term nitrogen fertilization on fungi was also evaluated. Samples of rhizosphere (soil surrounding plant roots) and biological soil crust (BSC) were collected in central New Mexico, USA. DNA was amplified from the samples with fungal specific primers. Twelve clone libraries were generated with a total of 307 (78 Operational Taxonomic Units, OTUs) and 324 sequences (67 OTUs) for BSC and rhizosphere, respectively. Approximately 40\% percent of the soil OTUs were considered novel (less than 97\% identity when compared to other sequences in NCBI using BLAST. The dominant organisms were dark-septate (melanized fungi) ascomycetes belonging to the order Pleosporales. Effects of N enrichment on fungi were not evident at the community level; however the abundance of unique sequences, sampling intensity, and temporal variations may be uncovering the effect of N in composition and divesity of fungal communities. The fungal communities of rhizosphere soil and BSC overlapped substantially in composition, with a Jaccard abundance similarity index of 0.75. Further analyses are required to explore possible functions of the dominant species colonizing the different zones of semiarid grassland soils.},
	urldate = {2010-07-08},
	journal = {Mycologia},
	author = {Porras-Alfaro, Andrea and Herrera, Jose and Navig, Donald O. and Lipinski, Kendra and Sinsabaugh, Robert},
	month = jun,
	year = {2010},
	pages = {09--297}
}

@article{de_castro_diversity_2008-2,
	title = {Diversity of soil fungal communities of {Cerrado} and its closely surrounding agriculture fields},
	volume = {190},
	issn = {0302-8933},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/18458875},
	doi = {10.1007/s00203-008-0374-6},
	abstract = {Cerrado is a savanna-like region that covers a large area of Brazil. Despite its biological importance, the Cerrado has been the focus of few microbial diversity studies. A molecular approach was chosen to characterize the soil fungal communities in four areas of the Cerrado biome: a native Cerrado, a riverbank forest, an area converted to a soybean plantation, and an area converted to pasture. Global diversity of fungal communities in each area was assessed through Ribosomal intergenic spacer analysis which revealed remarkable differences among the areas studied. Sequencing of approximately 200 clones containing 18S rDNA sequences from each library was performed and, according to the genetic distance between sequences, these were assigned to operational taxonomic units (OTUs). A total of 75, 85, 85, and 70 OTUs were identified for the native Cerrado, riverbank forest, pasture, and soybean plantation, respectively. Analysis of sequences using a similarity cutoff value of 1\% showed that the number of OTUs for the native Cerrado area was reduced by 35\%; for the soybean plantation, a reduction by more than 50\% was observed, indicating a reduction in fungal biodiversity associated with anthropogenic activity. This is the first study demonstrating the anthropogenic impact on Cerrado soil fungal diversity.},
	number = {2},
	urldate = {2010-07-08},
	journal = {Archives of Microbiology},
	author = {de Castro, Alinne Pereira and Quirino, Betania Ferraz and Pappas, Georgios and Kurokawa, Adriane Silva and Neto, Eduardo Leonardecz and Krüger, Ricardo Henrique},
	month = aug,
	year = {2008},
	pmid = {18458875},
	keywords = {Agriculture, Biodiversity, Brazil, DNA, Fungal, DNA, Ribosomal, Fungi, Molecular Sequence Data, Phylogeny, RNA, Ribosomal, 18S, Sequence Analysis, DNA, Soil, Soil Microbiology},
	pages = {129--139}
}

