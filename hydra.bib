	month = mar,
	year = {2010},
	pmid = {20130030},
	keywords = {Base Sequence, Computational Biology, Databases, Genetic, Genome, Bacterial, Metagenome, Metagenomics, Molecular Sequence Data, Sequence Analysis, DNA},
	pages = {715--721}
}

