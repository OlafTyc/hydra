	keywords = {Carbohydrate Metabolism, Carrier Proteins, Databases, Protein, Esterases, Glycoconjugates, Glycomics, Glycoside Hydrolases, Glycosyltransferases, Polysaccharide-Lyases},
	pages = {D233--238}
}

