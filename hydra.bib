@article{tyler_phytophthora_2006,
	title = {Phytophthora genome sequences uncover evolutionary origins and mechanisms of pathogenesis},
	volume = {313},
	issn = {1095-9203},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/16946064},
	doi = {10.1126/science.1128796},
	abstract = {Draft genome sequences have been determined for the soybean pathogen Phytophthora sojae and the sudden oak death pathogen Phytophthora ramorum. Oömycetes such as these Phytophthora species share the kingdom Stramenopila with photosynthetic algae such as diatoms, and the presence of many Phytophthora genes of probable phototroph origin supports a photosynthetic ancestry for the stramenopiles. Comparison of the two species' genomes reveals a rapid expansion and diversification of many protein families associated with plant infection such as hydrolases, ABC transporters, protein toxins, proteinase inhibitors, and, in particular, a superfamily of 700 proteins with similarity to known oömycete avirulence genes.},
	number = {5791},
	urldate = {2009-09-02},
	journal = {Science (New York, N.Y.)},
	author = {Tyler, Brett M and Tripathy, Sucheta and Zhang, Xuemin and Dehal, Paramvir and Jiang, Rays H Y and Aerts, Andrea and Arredondo, Felipe D and Baxter, Laura and Bensasson, Douda and Beynon, Jim L and Chapman, Jarrod and Damasceno, Cynthia M B and Dorrance, Anne E and Dou, Daolong and Dickerman, Allan W and Dubchak, Inna L and Garbelotto, Matteo and Gijzen, Mark and Gordon, Stuart G and Govers, Francine and Grunwald, Niklaus J and Huang, Wayne and Ivors, Kelly L and Jones, Richard W and Kamoun, Sophien and Krampis, Konstantinos and Lamour, Kurt H and Lee, Mi-Kyung and McDonald, W Hayes and Medina, Mónica and Meijer, Harold J G and Nordberg, Eric K and Maclean, Donald J and Ospina-Giraldo, Manuel D and Morris, Paul F and Phuntumart, Vipaporn and Putnam, Nicholas H and Rash, Sam and Rose, Jocelyn K C and Sakihama, Yasuko and Salamov, Asaf A and Savidor, Alon and Scheuring, Chantel F and Smith, Brian M and Sobral, Bruno W S and Terry, Astrid and Torto-Alalibo, Trudy A and Win, Joe and Xu, Zhanyou and Zhang, Hongbin and Grigoriev, Igor V and Rokhsar, Daniel S and Boore, Jeffrey L},
	month = sep,
	year = {2006},
	pmid = {16946064},
	keywords = {Algal Proteins, DNA, Algal, Evolution, Genes, Genome, Hydrolases, Photosynthesis, Phylogeny, Physical Chromosome Mapping, Phytophthora, Plant Diseases, Polymorphism, Single Nucleotide, Repetitive Sequences, Nucleic Acid, Sequence Analysis, DNA, Symbiosis, Toxins, Biological},
	pages = {1261--1266}
