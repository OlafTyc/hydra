	month = oct,
	year = {2016},
	note = {00000},
	keywords = {BAM, DNA sequence, FASTA, FASTQ, Multi-FASTA, protein sequence, reference-based compression, reference-free compression, SAM},
	pages = {56},
	file = {Hosseini_et_al_2016_A_Survey_on_Data_Compression_Methods_for_Biological_Sequences.pdf:/home/mattias/stack/zotero_papers//2016/Hosseini_et_al_2016_A_Survey_on_Data_Compression_Methods_for_Biological_Sequences.pdf:application/pdf;Snapshot:/home/mattias/.mozilla/firefox/88hqpn07.default/zotero/storage/76PIFZ2T/htm.html:text/html}
